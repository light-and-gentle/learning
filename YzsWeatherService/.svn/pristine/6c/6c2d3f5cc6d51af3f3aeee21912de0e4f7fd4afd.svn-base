/*农气*/
/*农情情报*/

.info-classify{
	width: 100%;
	height: 140px;
	background-color: #f2f2f2;
	border-bottom: solid 1px #ccc;
}
.classify-list{
	width: 92%;
	padding: 11px 0;
	margin: 0 auto;
}
.classify-list-content{
	margin-right: -20px;
}
.classify-list-content li{
	float: left;
	width: 15%;
	height: 118px;
	margin-right: 20px;
	/*background-size: contain;*/
	background-size: 100% 118px;
	cursor: pointer;
}
.classify-list ul .classify-active,
.classify-list ul li:hover{
	background-size: 100% 118px;
	cursor: pointer;
}
.classify-list ul li:last-child{
	margin-right: 0;
}
.xb{
	background: url("../img/xb.png") no-repeat;
}
.cbzb{
	background: url("../img/cbzb.png") no-repeat;
}
.qsqz{
	background: url("../img/qsqz.png") no-repeat;
}
.ghjc{
	background: url("../img/ghjc.png") no-repeat;
}
.qhyx{
	background: url("../img/qhyx.png") no-repeat;
}
.xb:hover,
.classify-active.xb{
	background: url("../img/h-xb.png") no-repeat;
}
.cbzb:hover,
.classify-active.cbzb{
	background: url("../img/h-cbzb.png") no-repeat;
}
.qsqz:hover,
.classify-active.qsqz{
	background: url("../img/h-qsqz.png") no-repeat;
}
.ghjc:hover,
.classify-active.ghjc{
	background: url("../img/h-ghjc.png") no-repeat;
}
.qhyx:hover,
.classify-active.qhyx{
	background: url("../img/h-qhyx.png") no-repeat;
}

.info-list{
	margin: 18px 10px 0;
	background: #f7f7f7;
	padding-bottom: 20px;
}
.info-title-content{
	height: 50px;
	border: solid 1px #ccc;
	background: #f7f7f7;
}
.info-title{
	line-height: 50px;
	color: #333;

}
.search-content{
	width: 300px;
	height: 34px;
	margin-top: 6px;
	margin-right: 27px;
	border: solid 1px rgba(9,140,240,0.54);
	box-shadow: 0px 1px 0px 0px
	rgba(255, 255, 255, 0.65),
	inset 0px 1px 2px 0px
	rgba(0, 0, 0, 0.15);
	border-radius: 20px;
	background: #eee;
}
.search-t{
	float: left;
	width: 248px;
	font-size: 14px;
	padding-left: 17px;
	line-height: 34px;
	background: #eee;
	border-radius: 20px;
}
.search-btn{
	float: right;
	width: 32px;
	height: 32px;
	border-radius: 50%;
	box-shadow: 0px 1px 1px 0px
	rgba(0, 0, 0, 0.15),
	inset 0px 1px 0px 1px
	#ffffff;
	background: url("../img/sousuo.png")
	no-repeat center center #f4f4f4;
	cursor: pointer;
}
.info-doc-list{
	line-height: 60px;
}
.info-doc-list li{
	position: relative;
	padding-left: 25px;
	border-bottom: 1px dashed #d8d8d8;
	font-size: 14px;
	line-height: 40px;
	background: url("../img/nqfw.png")no-repeat center left ;
	cursor: pointer;
}
.info-doc-list li:hover{
	color: #1f72ba;
	background: url("../img/nqfw.png")no-repeat center left  #fff;
}
.time{
	position: absolute;
	right: 40px;
	top: 5px;
	font-size: 14px;
	line-height: 30px;
}
