body{
	font-size: 12px;
}
.header{
	min-width: 1340px;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 99999;
	width: 100%;
	height: 75px;
	background-color: #009577;
}
.logo{
	width:32.5%;
	float: left;
	margin-left: 3.1%;
	margin-top: 7px;
}
.logo img{
	width: 100%;
}
.btn-nav{
	float: right;
	margin: 14px 26px 0 46px;
}
.btn-nav a{
	display: inline-block;
	width: 47px;
	height: 47px;
	background-size: cover;
}
.qr-code{
	margin-right: 20px;
	background: url("../img/qr-code.png") no-repeat;
}
.tc{
	background: url("../img/serve.png") no-repeat;
}
.menu-nav{
	font-size: 0;
	float: right;
}
.menu-nav a{
	display: inline-block;
	font-size: 24px;
	line-height: 75px;
	padding: 0 36px;
	text-decoration: none;
	color: #fff;
}
.menu-nav .menu-nav-active ,
.menu-nav a:hover{
	color: #a2facb;
	background: #103e35;
}

/*内容*/
.warp{
	min-width: 1300px;
	position: absolute;
	top: 85px;
	left: 20px;
	right: 20px;
	border-bottom: 0;
	box-sizing: border-box;
	/*min-height: 850px;*/
}
.sidebar{
	position: absolute;
	top: 0;
	left: 0;
	bottom: 0;
	float: left;
	width: 16.15%;
	background: #16584b;
}
.sidebar img{
	display: block;
	width: 100%;
}
.menu-sidebar{
	margin-top: 39px;
	font-size: 16px;
	line-height: 48px;
	color: #fff;
}
.menu-sidebar li{
	margin-top: 10px;
	text-align: center;
	cursor: pointer;
}
.menu-sidebar i{
	display: inline-block;
	vertical-align: middle;
	width: 24px;
	height: 24px;
	margin-right: 16px;
}
.menu-sidebar a{
	text-decoration: none;
	color: #fff;
}
.menu-sidebar li:hover,
.menu-sidebar-active{
	background: #103e35;
}
.menu-sidebar li:hover a,
.menu-sidebar-active a{
	color: #a2facb;
}
.menu-sidebar li:hover i,
.menu-sidebar-active i{
	background: url("../img/s-active.png") no-repeat;
	background-size: cover;
}
.main-content{
	box-sizing: border-box;
	float: right;
	/*height: 950px;*/
	width: 83%;
	border: 5px solid #16584b;
	border-bottom: none;
}
.content-top{
	box-sizing: border-box;
	height: 49px;
	background: #f2f2f2;
	border: solid 1px #ccc;
}
.select-area{
	float: left;
	position: relative;
	left: 0;
	top: 0
}
.qx{
	width: 120px;
	height: 48px;
	line-height: 48px;
	color: #fff;
	text-align: center;
	background: url("../img/sub-bg.png") no-repeat;
	background-size: cover;
	cursor: pointer;
}
.current-area{
	margin-right: 8px;
}
.qx-icon{
	display: inline-block;
}
.triangle-up {
	width: 0;
	height: 0;
	border: 5px solid transparent;
	border-bottom: 7px solid #fff;
}
.triangle-down {
	vertical-align: middle;
	width: 0;
	height: 0;
	border: 5px solid transparent;
	border-top: 7px solid #fff;
}
/*区县选择框*/
.area-content{
	display: none;
	position: absolute;
	width: 241px;
	height: 184px;
	top: 55px;
	left: 5px;
	padding: 12px;
	background: #f4f4f4;
	border: solid 1px #c8c8c8;
	border-radius: 4px;
	box-shadow: 0 1px 4px 0 rgba(0, 0, 0, 0.17);
	z-index: 2;
}
.area-content ol li{
	float: left;
	width:80px;
	color: #646464;
	text-align: center;
	line-height:36px;
	cursor: pointer;
}
.area-content ol .area-active{
	background: url("../img/qx-active.png") no-repeat 0 center;
}
.zd-content{
	float: right;
	width: 90%;
}
.arrow{
	float: left;
	display: none;
	margin-top: 12px;
	cursor: pointer;
}
.zd-name-content{
	position: relative;
	width: 90%;
	left: 0;
	top: 0;
	float: left;
	overflow: hidden;
}
.zd-name{
	position: relative;
	left: 0;
	top: 0;
	font-size: 0;
	white-space: nowrap
}
.zd-name li{
	display: inline-block;
	padding: 0 11px;
	margin-left: 20px;
	font-size:14px;
	line-height: 47px;
	cursor: pointer;
}
.zd-name li:hover,
.zd-name .zd-active{
	position: relative;
	left: 0;
	top: 0;
	background: #ababab;
	color: #fff;
	border-bottom: solid 2px #f69945;
}
.zd-name li:hover:after,
.zd-name .zd-active:after{
	content: '';
	position: absolute;
	left: 50%;
	bottom: 0;
	margin-left: -6px;
	border: 6px solid transparent;
	border-bottom-color: #f69945;
}
.arrow-l{
	float: left;
	margin-left: 15px;
	width: 22px;
	height: 22px;
	background: url("../img/arrow-l.png") no-repeat;
}
.arrow-r{
	float: right;
	margin-right: 10px;
	width: 22px;
	height: 22px;
	background: url("../img/arrow-r.png") no-repeat;
}
.content{
	box-sizing: border-box;
	margin: 6px 8px 0;
	background: #f2f2f2;
}
.content-l{
	float: left;
	width: 79%;
}
.content-r{
	float: right;
	width: 20%;
}
.sub-title{
	padding-left: 30px;
	font-size: 14px;
	line-height: 32px;
	color: #666;
}

.bt{
	border-top: solid 3px #009577;
}
.title-content{
	width: 100%;
	height: 35px;
	background:#f2f2f2;
}
.mt18{
	margin-top: 18px;
}
.bs{
	box-shadow: 0 2px 3px 0 rgba(0, 0, 0, 0.15);
}
.fl{
	float: left;
}
.fr{
	float: right;
}
.fs16{
	font-size: 16px;
}

/*自定义分页器*/
/*包裹自定义分页器的div的位置等CSS样式*/
.swiper-pagination-custom {
	bottom: 10px;
	left: 0;
	width: 100%;
}
/*自定义分页器的样式，这个你自己想要什么样子自己写*/
.swiper-pagination-customs {
	width: 30px;
	height: 4px;
	display: inline-block;
	background: #000;
	opacity: .3;
	margin: 0 5px;
}
/*自定义分页器激活时的样式表现*/
.swiper-pagination-customs-active {
	opacity: 1;
	background-color: #F78E00;
}

/* 二维码 */
.code{
	width: 200px;
	right: 13px;
	top:75px;
	position: absolute;
	z-index: 999;
	display: none;
}
@media only screen and (max-width: 1366px) {
	.header {
		height: 59px;
	}
	.warp{
		top: 67px;
	}
	.btn-nav {
		margin: 12px 18px 0 32px;
	}
	.btn-nav a{
		width: 35px;
		height: 35px;
	}
	.qr-code {
		margin-right: 16px;
	}

	.menu-nav a {
		line-height: 59px;
		font-size: 16px;
		padding: 0 26px;
	}

	.menu-sidebar {
		display: none;
		margin-top: 24px;
		line-height: 38px;
	}
	.menu-sidebar li{

	}
	.menu-sidebar i{
		width: 18px;
		height: 18px;
		margin-right: 16px;
	}
	.main-content{
		height: 690px;
	}
	.content-top{
		height: 36px;
	}
	.qx{
		width: 97px;
		height: 34px;
		line-height: 34px;
	}
	.area-content{
		top: 40px;
	}
	.arrow{
		margin-top: 6px;
	}
	.zd-name li{
		line-height: 33px;
	}
	.code{
		top: 59px;
	}
}
