
/*气象观测*/
.weather-content{
	padding: 5px 40px 0;
}
.today{
	height: 269px;
}
.today .sk i,
.therm{
	background-image: url(../img/iconall.png);
	background-repeat: no-repeat;
	display: block;
}
.sk{
	float: left;
	width: 34%;
	height: 100%;
	position: relative;
	background-color: #ffffff;
	border-radius: 4px;
	box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.35);
}
.time{
	position: absolute;
	left: 26px;
	top: 13px;
	font-size: 13px;
	color: #7e7e7e;
}
.zs{
	position: absolute;
}
.zs i {
	float: left;
	height: 24px;
	margin-right: 8px;
	width: 24px;
}
.zs span {
	float: left;
	font-size: 16px;
	letter-spacing: 0;
	margin-right: 8px;
}
.zs em {
	float: left;
	font-size: 16px;
}
.h{
	left: 24px;
	top: 214px;
}
.h i {
	background-position: -2px -289px;
}
.w {
	left: 200px;
	top: 213px;
}
.w i {
	background-position: -36px -291px;
}
.today .sk .tem{
	color: #2d76b6;
	left: 149px;
	position: absolute;
	top: 102px;
}
.today .sk .tem span {
	font-size: 62px;
}
.today .sk .tem em {
	font-size: 50px;
}
.therm {
	background-position: 0 0;
	height: 132px;
	left: 62px;
	position: absolute;
	top: 61px;
	width: 76px;
}
.therm p {
	bottom: 35px;
	left: 35px;
	position: absolute;
	width: 15px;
}
.therm p .t {
	background-position: -35px -137px;
	height: 8px;
	width: 15px;
}
.therm p .c {
	background-position: -35px -142px;
	height: 38px;
	width: 15px;
}
.today ul {
	position: relative;
	float: left;
	width: 30%;
	height: 100%;
	z-index: 0;
}
.today ul li{
	position: relative;
	float: left;
	width: 47%;
	height: 100%;
	margin-left: 7px;
	font-size: 14px;
	text-align: center;
	background-color: #ffffff;
	border-radius: 4px;
	box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.35);
}
.today ul li h1 {
	font-size: 13px;
	margin-top: 20px;
	line-height: 18px;
}
div.jpg80{
	height: 80px;
	width: 80px;
	margin: 0 auto;
	background-repeat: no-repeat;
	background-size: cover;
}
.n00{
	background: url("../wIcon/0.png") no-repeat;
}
.n01{
	background: url("../wIcon/1.png") no-repeat;
}
.wea {
	height: 36px;
	line-height: 18px;
	margin: 0 auto;
	overflow: hidden;
	padding-top: 0;
	width: 88%;
}
.sky {
	z-index: 2;
	/*margin-top: 10px;*/
	position: relative;
	width: 100%;
	height: 22px;
}
.today ul li .tem {
	color: #7f7f7f;
	margin-top: 10px;
	line-height: 30px;
}
.today ul li .tem span {
	font-size: 36px;
}
.today ul li .tem em {
	font-size: 24px;
}
.today ul li .win {
	display: inline-block;
	height: 20px;
	line-height: 20px;
	margin-top: 7px;
	overflow: hidden;
	text-align: center;
}
.today ul li .win i {
	display: inline-block;
	float: left;
	height: 25px;
	width: 24px;
	background-repeat: no-repeat;
}
.win i.NW{
	background: url("../dataImg/f319.png") no-repeat;
}
.win i.SE{
	background: url("../dataImg/f126.png") no-repeat;
}
.show-img{
	float: right;
	width: 35.55%;
	height: 100%;
	border-radius: 4px;
	box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.35);
}
.show-img img{
	width: 100%;
	height: 100%;
	border-radius: inherit;
}
.tab-content{
	box-sizing: border-box;
	width: 100%;
	/*height: 520px;*/
	margin-top: 22px;
	background: #fff;
}
.tab-pane{
	width: 100%;
	padding: 0 14px;
	font-size: 16px;
	line-height: 45px;
	font-weight: bold;
	color: #666;
	border-bottom: solid 2px #f2f2f2;
}
.tab-pane li{
	box-sizing: border-box;
	display: inline-block;
	padding: 0 16px;
	cursor: pointer;
}
.tab-pane li:hover,
.tab-pane .tab-active{
	background: #b2b2b2;
	color: #fff;
	border-left: solid 1px #f2f2f2;
	border-right: solid 1px #f2f2f2;
}
.chart-content{
	padding: 0 14px;
	margin-top: 40px;
	height: 370px;
}
.chart-info{
	width: 100%;
	height: 100%;

}
/*小气候观测1*/
.microclimate-content{
	margin-top: 10px;
	background: #fff;
	padding-bottom: 10px;
}

.sub-title{
	padding-left: 30px;
	font-size: 14px;
	line-height: 32px;
}
.content-l{
	float: left;
	width: 79%;
}
.mark-content li{
	float: left;
	font-size: 14px;
	line-height: 32px;
	margin-right: 20px;
}
.color-icon{
	display: inline-block;
	width: 22px;
	height: 10px;
	margin-right: 6px;
	border-radius: 4px;
}
.icon-228b22{
	background: #228b22;
}
.icon-48b{
	background: #48b;
}
.icon-ff4500{
	background: #ff4500;
}
.panel-content{
	width: 100%;
	height: 833px;
}
.panel{
	float: left;
	width: 31.75%;
	height: 259px;
	margin: 10px;
	background: url("../img/panel-bg.png") no-repeat;
	-webkit-background-size: 100% 259px;
	background-size: 100% 259px;

}
.panel div.meter{
	width: 100%;
	height: 100%;
}
.content-r{
	float: right;
	width: 20%;
}
.device-content{
	box-sizing: border-box;
	padding-top: 8px;
	height: 214px;
}
.device-content p{
	line-height: 18px;
	padding-left:12px;
}
.live-content{
	height: 255px;
}
.live-content video{
	width: 100%;
	height: 100%;
}
.site-content{
	height: 248px;
}
/*自定义分页器*/
/*包裹自定义分页器的div的位置等CSS样式*/
.swiper-pagination-custom {
	bottom: 10px;
	left: 0;
	width: 100%;
}
/*自定义分页器的样式，这个你自己想要什么样子自己写*/
.swiper-pagination-customs {
	width: 30px;
	height: 4px;
	display: inline-block;
	background: #000;
	opacity: .3;
	margin: 0 5px;
}
/*自定义分页器激活时的样式表现*/
.swiper-pagination-customs-active {
	opacity: 1;
	background-color: #F78E00;
}
/*小气候观测2*/
.sub-content-l{
	width: 70px;
	background: #3b98ef;
}
.icon-content{
	height: 18px;
	box-sizing: border-box;
	border: solid 1px #94b2ce;
	background: #294763;
}
.icon-content .icon{
	display: block;
	width: 25px;
	height: 6px;
	margin: 5px auto;
	background: url("../img/icon1.png") no-repeat;
}
.param{
	margin-top: 16px;
	width: 100%;
}
.param li{
	text-align: center;
	margin-bottom: 8px;
	padding: 12px 10px;
	font-size: 14px;
	line-height: 1;
	color: #fff;
	cursor: pointer;
}
.param p{
	margin-top: 6px;
	padding: 0 8px;
}
.param li:hover,
.param .param-active{
	background: #4f8ac2;
}
.param i{
	display: block;
	width: 50px;
	height: 50px;
	margin: 0 auto;
}
.temp{
	background: url("../img/temp.png") no-repeat;
}
.rh{
	background: url("../img/rh.png") no-repeat;
}
.st{
	background: url("../img/st.png") no-repeat;
}
.sh{
	background: url("../img/sh.png") no-repeat;
}
.fain-fall{
	background: url("../img/fain-fall.png") no-repeat;
}
.pre{
	background: url("../img/pre.png") no-repeat;
}
.eff{
	background: url("../img/eff.png") no-repeat;
}
.winds{
	background: url("../img/winds.png") no-repeat;
}
.wind-dir{
	background: url("../img/wind-dir.png") no-repeat;
}
.sub-content-r{
	width: 90%;
	margin-right: 30px;
}
.sub-chart-content{
	padding-bottom: 30px;
}
.chart-title{
	font-size: 18px;
	line-height: 24px;
	font-weight: bold;
	text-indent: 2em;
	color: #666;
	margin: 20px 0;
}
.line-chart-info{
	width: 90%;
	height: 310px;
	margin: 0 auto;
}
@media only screen and (max-width: 1366px) {
	.tab-content{
		/*height: 370px;*/
	}
	.panel-content{
		height: 607px;
	}
	.panel{
		color: #87cefa;
		width: 32%;
		height: 192px;
		margin: 5px;
		-webkit-background-size: 100% 187px;
		background-size: 100% 192px;
	}
	.device-content{
		height: 154px;
	}
	.live-content{
		height: 162px;
	}
	.site-content{
		height: 177px;
	}
}