/*农情*/
.nq-title{
	line-height: 35px;
	border: solid 1px #ccc;
	position: relative;
	left:0;
	top: 0;
	color: #333;
}
.nq-title i{
	display: inline-block;
	vertical-align: middle;
}
.icon-h{
	width: 20px;
	height: 18px;
	margin: 0 18px;
	background: url("../img/home.png") no-repeat;
}
.icon-j{
	width: 10px;
	height: 16px;
	margin: 0 10px;
	background: url("../img/jiantou.png") no-repeat;
}
.para-info-content{
	float: right;
	position: relative;
	top: 15px;
	right: 15px;
	box-sizing: border-box;
	width: 255px;
	padding-left: 10px;
	line-height: 28px;
	font-size: 14px;
	z-index: 999;
	background-color: white;
 }
.pare-title{
	font-weight: bold;
	line-height: 42px;
	color: #020202;
}
.handle{
	padding-bottom: 10px;
}
.basics li{
	float: left;
	box-sizing: border-box;
	width: 85px;
	padding-left: 6px;
	margin-bottom: 7px;
	margin-right: 7px;
	/*text-align: center;*/
	background: #e7e7e7;
 }
.basics .w142{
	width:142px;
}
.yujing li{
	float: left;
	width: 75px;
	height: 28px;
	margin-right: 5px;
	margin-bottom: 8px;
	line-height: 28px;
	text-align: center;
	background: #e7e7e7;
}
.yujing .w155{
	width: 155px;
}
.yujing .w115{
	width: 115px;
}
.map-content{
	position:absolute;
	width: 100%;
	height: 100%;
}