/*设置预警InfoWindow*/
.alertinfoWin{
	width:500px !important;
	height:300px;
	overflow:scroll;
}

/*农情*/

/*检测预警*/
.nq-title{
	line-height: 35px;
	border: solid 1px #ccc;
	position: relative;
	left:0;
	top: 0;
	color: #333;
}
.para-info-content{
	float: right;
	position: relative;
	top: 15px;
	right: 15px;
	box-sizing: border-box;
	width: 255px;
	padding-left: 10px;
	line-height: 28px;
	font-size: 14px;
	z-index: 999;
	background-color: white;
 }
.pare-title{
	font-weight: bold;
	line-height: 42px;
	color: #020202;
}
.handle{
	padding-bottom: 10px;
}
.basics li{
	float: left;
	box-sizing: border-box;
	width: 85px;
	padding-left: 6px;
	margin-bottom: 7px;
	margin-right: 7px;
	/*text-align: center;*/
	background: #e7e7e7;
 }
.basics .w142{
	width:142px;
}
.yujing li{
	float: left;
	width: 75px;
	height: 28px;
	margin-right: 5px;
	margin-bottom: 8px;
	line-height: 28px;
	text-align: center;
	background: #e7e7e7;
}
.yujing .w155{
	width: 155px;
}
.yujing .w115{
	width: 115px;
}
.map-content{
	position:absolute;
	width: 100%;
	height: 100%;
}

/*区县产品*/
.product-content{
	margin-top: 10px;
	background: #fff;
	padding-bottom: 10px;
}
.detail-content{
	padding-top: 11px;
}
.info-content{
	padding-right: 10px;
}
.nq-cp-title{
	display: inline-block;
	padding: 0 11px;
	height: 25px;
	font-size:14px;
	line-height: 25px;
	background: #6fb9fe;
	color: #f4f4f4;
	border-radius: 0 10px 10px 0;
}
.detail-info{
	text-indent: 2em;
	padding-left: 10px;
	padding-top: 10px;
	font-size: 14px;
	line-height: 24px;
}
.feature-text{
	width: 55%;
}
.feature-img{
	width: 345px;
	height: 193px;
	border-radius: 4px;
}
.feature-img img{
	width: 100%;
	height: 100%;
	border-radius: 4px;
}
.area-map-content{
	width:100%;
	height: 200px;
}
.yz-distribution{
	width: 100%;
	height: 220px;
}
.yz-distribution img{
	width: 100%;
	height: 100%;
}
.check-btn{
	height: 28px;
	padding-top: 2px;
}
.check-btn span {
	display: inline-block;
	float: right;
	height: 26px;
	padding-left: 22px;
	margin-right: 10px;
	font-size: 14px;
	line-height: 26px;
	background:url("../img/xq.png") no-repeat 0 center;
	cursor: pointer;
}
.price-content{
	max-height: 230px;
}
.price-table{
	width: 100%;
	max-width: 100%;
}
.price-table thead{
	height: 24px;
	line-height: 24px;
	text-align: center;
	background: #00b7ee;
}
.price-table th{
	color: #fff;
}
.price-table tbody td{
	text-align: center;
	border: 1px solid #e9e9e9;
}
.qh-distribution{
	width: 100%;
	height: 280px;
}
.qh-distribution img{
	width: 100%;
	height: 100%;
}
.tanchu{
	display: none;
	width: 580px;
	height: 460px;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-top: -200px;
	margin-left: -310px;
}
.zhezhao{
	display: none;
	position: absolute;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	background: rgba(0,0,0,.5);
}
.close{
	width: 50px;
	height: 50px;
}
.close img{
	width: 100%;
	height: 100%;
}
.img-show-content{
	position: absolute;
	left: 50%;
	top: 50%;
	margin-left: -290px;
	margin-top: -320px;
	width: 557px;
	height: 431px;
}
.img-show-content img{
	width: 100%;
	height: 100%;
}